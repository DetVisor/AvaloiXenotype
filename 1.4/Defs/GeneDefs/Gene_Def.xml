<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef ParentName="GenePollutionBase">
    <defName>DV_ToxResist_Terrible</defName>
    <label>tox weakness</label>
    <labelShortAdj>absorbant</labelShortAdj>
    <description>Carriers of this gene are weaker to environmental toxins. Their skin and lungs absorbs toxic gases and takes more damage from tox gas, polluted terrain, and toxic fallout. They also build up rot stink exposure faster.</description>
    <iconPath>UI/Genes/Gene_ToxWeakness</iconPath>
    <displayOrderInCategory>20</displayOrderInCategory>
    <statOffsets>
      <ToxicResistance>-0.3</ToxicResistance>
    </statOffsets>
    <biostatMet>2</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li>
          <symbol>tox</symbol>
          <weight>2</weight>
        </li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

  <GeneDef ParentName="GeneJawBase">
    <defName>DV_Head_Avaloi</defName>
    <label>three-eyed head</label>
    <description>Carriers of this gene have a smooth head with 2 normal eyes and a third, less useful eye on their forehead.</description>
    <iconPath>UI/Icons/Genes/Gene_GauntHead</iconPath>
    <forcedHeadTypes>
      <li>DV_Avaloi_AverageNormal</li>
    </forcedHeadTypes>
    <displayOrderInCategory>97</displayOrderInCategory>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>alien</symbol></li>
        <li><symbol>three-eyed</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

  <GeneDef ParentName="GeneNoseBase">
    <defName>DV_Coral_Beards</defName>
    <label>coral fans</label>
    <description>Carriers of this gene grow beard-like appendages on their cheeks which can have various patterns and shapes.</description>
    <iconPath>UI/Genes/Gene_CoralBeard</iconPath>
    <graphicData>
      <graphicPaths>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard1</li>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard2</li>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard3</li>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard4</li>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard5</li>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard6</li>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard7</li>
        <li>Things/Pawn/Humanlike/HeadAttachments/CoralBeards/CoralBeard8</li>
       </graphicPaths>
      <narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset>
      <colorType>Hair</colorType>
      <layer>PostHeadgear</layer>
      <drawOffsetNorth>(0, -0.3, 0.1)</drawOffsetNorth>
      <drawLoc>HeadTop</drawLoc>
      <visibleNorth>true</visibleNorth>
    </graphicData>
    <missingGeneRomanceChanceFactor>0.2</missingGeneRomanceChanceFactor>
    <displayCategory>Miscellaneous</displayCategory>
    <displayOrderInCategory>410</displayOrderInCategory>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>pig</symbol></li>
        <li><symbol>pork</symbol></li>
        <li><symbol>snout</symbol></li>
        <li><symbol>boar</symbol></li>
        <li><symbol>hog</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>pig</symbol></li>
        <li><symbol>snout</symbol></li>
        <li><symbol>swine</symbol></li>
        <li><symbol>boar</symbol></li>
        <li><symbol>hog</symbol></li>
        <li><symbol>nose</symbol></li>
      </suffixSymbols>
    </symbolPack>
  </GeneDef>

